    <ImageView
        android:id="@+id/hamburgerMenuIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/hamburger_menu_icon" />


</androidx.appcompat.widget.Toolbar>